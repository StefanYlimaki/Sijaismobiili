{
  "expo": {
    "name": "Sijaismobiili",
    "slug": "Sijaismobiili",
    "owner":"sijaismobiili",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/ic_stat_logo_fblanc_512_large.png",
          "color": "#ffffff"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.proj.sijaismobiili",
      "versionCode": 2,
      "permissions": ["NOTIFICATIONS"],
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi":true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f9cdf2c5-692a-448d-bcf8-f5fa311e5ad2"
      }
    }
  }
}
